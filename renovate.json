{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "groupName": "all dependencies",
  "separateMajorMinor": false,
  "groupSlug": "all",
  "packageRules": [
    {
      "matchPackagePatterns": [
        "*"
      ],
      "groupName": "all dependencies",
      "groupSlug": "all"
    },
    {
      "matchManagers": ["dockerfile"],
      "matchUpdateTypes":"patch",
      "enabled": false
    }
  ],
  "lockFileMaintenance": {
    "enabled": false
  },
  "platformAutomerge": true
}
